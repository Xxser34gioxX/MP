
public class Moto extends Automovil{

	private int cc;
	public Moto(int matricula, int numeroRuedas, int kilometraje) {
		super(matricula, numeroRuedas, kilometraje);
	}
	
	@Override
	int cambioDeRuedas() {
		
		return 0;
	}

	public int getCc() {
		return cc;
	}

	public void setCc(int cc) {
		this.cc = cc;
	}
	

}
